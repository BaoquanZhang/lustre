KERNEL=linux-2.4.20-31.9.tar.gz
SERIES=rh-2.4.20
VERSION=2.4.20
EXTRA_VERSION=31.9_lustre.1.2.2
RHBUILD=1

BASE_ARCHS="i586"
BIGMEM_ARCHS=""
BOOT_ARCHS=""
JENSEN_ARCHS=""
SMP_ARCHS="i586"
UP_ARCHS=""
SRC_ARCHS="i586"

# the modules in this kernel do not build with gcc 3
for cc in i386-redhat-linux-gcc-2.96 gcc296 gcc ; do
    if which $cc >/dev/null 2>/dev/null ; then
        CC=$cc
        break
    fi
done
